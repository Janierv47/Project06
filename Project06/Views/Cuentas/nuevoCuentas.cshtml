
@{
    ViewBag.Title = "nuevoCuentas";
}

<h2>nuevoCuentas</h2>

<form id="frmNuevoCuentas" method="post">
    <div class="form-group">
        <label>Número de cuenta:</label>
        <input type="text" name="numeroCuenta" id="numeroCuenta" class="form-control" />
    </div>

    <div class="form-group">
        <label>ID del cliente:</label>
        <input type="text" name="id_cliente" id="id_cliente" class="form-control" />
    </div>

    <div class="form-group">
        <label>ID del tipo de cuenta:</label>
        <input type="text" name="id_tipoCuenta" id="id_tipoCuenta" class="form-control" />
    </div>

    <div class="form-group">
        <label>ID de la moneda:</label>
        <input type="text" name="id_moneda" id="id_moneda" class="form-control" />
    </div>

    <div class="form-group">
        <label>Saldo:</label>
        <input type="text" name="saldo" id="saldo" class="form-control" />
    </div>

    <div class="form-group">
        <label>Estado:</label>
        <input type="text" name="Estado" id="Estado" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/Cuentas/CuentasLista">Regresar a la lista</a>


</form>
