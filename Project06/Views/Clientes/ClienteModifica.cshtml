@model Project06.Models.sp_RetornaClienteID_Result

@{
    ViewBag.Title = "ClienteModifica";
}

<h2>ClienteModifica</h2>

<form id="frmModificaCliente" method="post">
    <div class="form-group">
        <label>Cédula:</label>
        <input type="text" name="cedula" id="cedula" class="form-control" value="@Model.cedula" />
    </div>

    <div class="form-group">
        <label>Género:</label>
        <input type="text" name="genero" id="genero" class="form-control" value="@Model.genero"/>
    </div>

    <div class="form-group">
        <label>Fecha de nacimiento:</label>
        <input type="text" name="fechaNacimiento" id="fechaNacimiento" class="form-control" value="@Model.fechaNacimiento"/>
    </div>

    <div class="form-group">
        <label>Nombre:</label>
        <input type="text" name="nombre" id="nombre" class="form-control" value="@Model.nombre"/>
    </div>

    <div class="form-group">
        <label>Primer apellido:</label>
        <input type="text" name="primerApellido" id="primerApellido" class="form-control" value="@Model.primerApellido"/>
    </div>

    <div class="form-group">
        <label>Segundo Apellido:</label>"
        <input type="text" name="segundoApellido" id="segundoApellido" class="form-control" value="@Model.segundoApellido"/>
    </div>

    <div class="form-group">
        <label>Dirección física:</label>
        <input type="text" name="direccionFisica" id="direccionFisica" class="form-control" value="@Model.direccionFisica"/>
    </div>

    <div class="form-group">
        <label>Teléfono principal:</label>
        <input type="text" name="telefonoprincipal" id="telefonoprincipal" class="form-control" value="@Model.telefonoPrincipal"/>
    </div>

    <div class="form-group">
        <label>Teléfono secundario:</label>
        <input type="text" name="telefonoSecundario" id="telefonoSecundario" class="form-control" value="@Model.telefonoSecundario"/>
    </div>

    <div class="form-group">
        <label>Correo:</label>
        <input type="text" name="correo" id="correo" class="form-control" value="@Model.correo"/>
    </div>

    <div class="form-group">
        <label>ID del tipo de cliente:</label>
        <input type="text" name="id_tipoCliente" id="id_tipoCliente" class="form-control" value="@Model.id_tipoCliente"/>
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/Clientes/ClientesLista">Regresar a la lista</a>


</form>