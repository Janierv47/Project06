
@model Project06.Models.sp_RetornaTipoCuentaID_Result
@{
    ViewBag.Title = "TipoCuentaModifica";
}

<form id="frmnuevoTipoCuenta" method="post">
    <div class="form-group">
        <label>Nombre de tipo de cuenta:</label>
        <input type="text" name="nombre" id="nombre" class="form-control" value="@Model.nombre" />
    </div>

    <div class="form-group">
        <label>Código:</label>
        <input type="text" name="codigo" id="codigo" class="form-control" value="@Model.codigo" />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/TipoCuenta/TipoCuentaLista">Regresar a la lista</a>
</form>

@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/scripts/TipoCuenta/Validaciones.js")
    @Scripts.Render("~/scripts/TipoCuenta/nuevoTipoCuenta.js")
}