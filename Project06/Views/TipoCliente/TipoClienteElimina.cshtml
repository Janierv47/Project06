@model Project06.Models.sp_RetornaTipoClienteID_Result
@{
    ViewBag.Title = "Elimina Tipo Cliente";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form id="frmNuevoTipoCliente" method="post">
    <input type="hidden" name="id_tipoCliente" id="id_tipoCliente" value="@Model.id_tipoCliente" />
    <div class="form-group">
        <label>Nombre:</label>
        <input type="text" name="nombre" id="nombre" class="form-control" value="@Model.nombre" readonly />
    </div>

    <div class="form-group">
        <label>Descripcion:</label>
        <input type="text" name="descripcion" id="descripcion" class="form-control" value="@Model.descripcion" readonly />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/TipoCliente/TipoClienteLista">Regresar a la lista</a>
</form>

@section Scripts{
    @* *****No es necesario incluir jquery porque
        ya el layout lo incluye*@
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/scripts/TipoCliente/Validaciones.js")
    @Scripts.Render("~/scripts/TipoCliente/nuevoTipoCliente.js")
}