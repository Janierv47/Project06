@model Project06.Models.sp_RetornaTipoClienteID_Result
@{
    ViewBag.Title = "TipoClienteModifica";
}

<form id="frmnuevoTipoCliente" method="post">
    <div class="form-group">
        <label>Nombre de tipo de cliente:</label>
        <input type="text" name="nombre" id="nombre" class="form-control" value="@Model.nombre" />
    </div>

    <div class="form-group">
        <label>Descripción:</label>
        <input type="text" name="descripcion" id="descripcion" class="form-control" value="@Model.descripcion" />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/TipoCliente/TipoClienteLista">Regresar a la lista</a>
</form>

@section scripts{
    
}

